@CHARSET "UTF-8";
.input_control {
    display: block;
    width: 100%;
    height: 100%;
    padding: 6px 12px;
    font-size: 14px;
    line-height: 1.42857143;
    color: #555;
    background-color: #fff;
    background-image: none;
    border: 0px;
    border-radius: 4px;
}

/**去掉下拉框背景三角形**/
select{
    appearance:none; 
    -webkit-appearance:none; 
}


.row_margin10{
	margin: 10px!important;
}