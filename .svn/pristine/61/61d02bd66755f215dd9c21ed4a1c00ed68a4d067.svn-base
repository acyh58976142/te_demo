.paginationhead {
  display: inline-flex;
  padding-left: 0;
  border-radius: 4px;
  width: 500px !important;
  margin-left:50% !important;
  margin-top: 20px !important;
  margin-bottom: 20px !important;
}
.paginationhead > li {
  display: inline;
}
.paginationhead > li > a,
.paginationhead > li > span {
  position: relative;
  float: left;
  padding: 6px 12px;
  margin-left: -1px;
  line-height: 1.42857143;
  color: #428bca;
  text-decoration: none;
  background-color: #fff;
  border: 1px solid #ddd;
}
.paginationhead > li:first-child > a,
.paginationhead > li:first-child > span {
  margin-left: 0;
  border-top-left-radius: 4px;
  border-bottom-left-radius: 4px;
}
.paginationhead > li:last-child > a,
.paginationhead > li:last-child > span {
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
}
.paginationhead > li > a:hover,
.paginationhead > li > span:hover,
.paginationhead > li > a:focus,
.paginationhead > li > span:focus {
  color: #2a6496;
  background-color: #eee;
  border-color: #ddd;
}
.paginationhead > .active > a,
.paginationhead > .active > span,
.paginationhead > .active > a:hover,
.paginationhead > .active > span:hover,
.paginationhead > .active > a:focus,
.paginationhead > .active > span:focus {
  z-index: 2;
  color: #fff;
  cursor: default;
  background-color: #428bca;
  border-color: #428bca;
}
.paginationhead > .disabled > span,
.paginationhead > .disabled > span:hover,
.paginationhead > .disabled > span:focus,
.paginationhead > .disabled > a,
.paginationhead > .disabled > a:hover,
.paginationhead > .disabled > a:focus {
  color: #999;
  cursor: not-allowed;
  background-color: #fff;
  border-color: #ddd;
}
.paginationhead-lg > li > a,
.paginationhead-lg > li > span {
  padding: 10px 16px;
  font-size: 18px;
}
.paginationhead-lg > li:first-child > a,
.paginationhead-lg > li:first-child > span {
  border-top-left-radius: 6px;
  border-bottom-left-radius: 6px;
}
.paginationhead-lg > li:last-child > a,
.paginationhead-lg > li:last-child > span {
  border-top-right-radius: 6px;
  border-bottom-right-radius: 6px;
}
.paginationhead-sm > li > a,
.paginationhead-sm > li > span {
  padding: 5px 10px;
  font-size: 12px;
}
.paginationhead-sm > li:first-child > a,
.paginationhead-sm > li:first-child > span {
  border-top-left-radius: 3px;
  border-bottom-left-radius: 3px;
}
.paginationhead-sm > li:last-child > a,
.paginationhead-sm > li:last-child > span {
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
}
.pager {
  padding-left: 0;
  margin: 20px 0;
  text-align: center;
  list-style: none;
}
.pager li {
  display: inline;
}
.pager li > a,
.pager li > span {
  display: inline-block;
  padding: 5px 14px;
  background-color: #fff;
  border: 1px solid #ddd;
  border-radius: 15px;
}
.pager li > a:hover,
.pager li > a:focus {
  text-decoration: none;
  background-color: #eee;
}
.pager .next > a,
.pager .next > span {
  float: right;
}
.pager .previous > a,
.pager .previous > span {
  float: left;
}
.pager .disabled > a,
.pager .disabled > a:hover,
.pager .disabled > a:focus,
.pager .disabled > span {
  color: #999;
  cursor: not-allowed;
  background-color: #fff;
}

a {outline: none;}
a:active {star:expression(this.onFocus=this.blur());}